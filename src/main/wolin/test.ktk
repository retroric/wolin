fun scopeTest(x: ubyte, y: ubyte, what: string, war: bool) {
    while (x < 100) {
        if(war) { // rzucić błąd, jeżeli w kondyszjonalsie jest esajnmęt
            while (y > 10) {
                x++
            }
        } else {
            while (x < 20) {
                y++
            }
        }
    }
}
